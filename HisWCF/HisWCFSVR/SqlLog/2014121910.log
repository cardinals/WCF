Select a.Zgid Ysdm,
       a.Xm Ysxm,
       a.Ksdm Ksdm,
       b.Ksmc Ksmc,
       c.Mc Yszc,
       (select yyyszc from WEB_GHYSXX where yyysdm = a.ysdm) Ystc,
       '' Ysjs
  From Gy_Zgxx a, Gy_Ksdm b, (Select Dm, Mc From Gy_Gydm Where Dmlb = 13) c
